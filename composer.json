{
    "name": "misantron/phpunit-guzzle-test-client",
    "description": "Guzzle http test client traits for PHPUnit",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^7.1",
        "phpunit/phpunit": "^8.5|^9.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "PHPUnit\\Guzzle\\TestClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPUnit\\Guzzle\\TestClient\\Tests\\": "tests"
        }
    }
}
