{
  "name": "misantron/phpunit-guzzle-test-client",
  "description": "Guzzle http test client helpers for PHPUnit",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksandr Ivanov",
      "email": "misantron@gmail.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "guzzlehttp/guzzle": "^6.5 || ^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5 || ^9.5",
    "symplify/easy-coding-standard": "^9.4"
  },
  "autoload": {
    "psr-4": {
      "PHPUnit\\Guzzle\\TestClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPUnit\\Guzzle\\TestClient\\Tests\\": "tests",
      "PHPUnit\\Guzzle\\TestClient\\Example\\": "examples"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "code-style": "vendor/bin/ecs check",
    "code-style-fixer": "vendor/bin/ecs check --fix"
  }
}
